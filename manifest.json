{
    "manifest_version": 3,
    "name": "Melanated Sanctuary Vision Board Extension",
    "version": "1.0",
    "permissions": [
      "storage", 
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon-16.ico",
      "48": "icons/icon-48.ico",
      "96": "icons/icon-96.ico",
      "128": "icons/icon-128.ico"
    }
  }
  